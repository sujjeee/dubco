{
  "name": "dubco",
  "version": "0.1.0",
  "description": "A CLI for easily shortening URLs using the dub.co service. ✂️",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "bin": {
    "dub": "./dist/index.js"
  },
  "preferGlobal": true,
  "license": "MIT",
  "author": {
    "name": "sujjeee",
    "url": "https://twitter.com/sujjeeee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sujjeee/dubco.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dub",
    "dubco",
    "dub.co",
    "cli",
    "url shortener",
    "sujjeee"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/index.js ",
    "lint": "biome lint --write . --unsafe",
    "format": "biome format --write .",
    "biome:ci": "biome ci .",
    "pub:release": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "configstore": "^6.0.0",
    "fs-extra": "^11.2.0",
    "json-colorizer": "^2.2.2",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "package-json": "^8.1.1",
    "prompts": "^2.4.2",
    "qrcode": "^1.5.4",
    "rimraf": "^5.0.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/configstore": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.16.10",
    "@types/prompts": "^2.4.9",
    "@types/qrcode": "^1.5.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2"
  }
}
